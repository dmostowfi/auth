<h1>Login</h1>

<!--this form is going to submit to the CREATE action of the SessionsController -->
<%= form_with :url => "/sessions" do |form| %>

<p>
<%= form.label "email" %>
<%= form.text_field "email" %>
<%= form.label "password" %>
<%= form.password_field "password" %>
</p>

<p>
<%= form.submit "Login"%>
</p>

<%end%>